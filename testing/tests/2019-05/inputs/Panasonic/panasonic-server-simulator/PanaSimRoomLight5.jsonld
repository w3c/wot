        "contentType": "application/json",
        "subprotocol": "longpoll",
        "op": "observeproperty",
        "security": ["nosec_sc"]
      }, {
        "href": "wss://w3c.p-wot.com:8003/wot/things/PanaSimRoomLight5/color",
        "contentType": "application/json",
