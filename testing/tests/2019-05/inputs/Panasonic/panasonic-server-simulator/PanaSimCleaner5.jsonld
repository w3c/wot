    "turnRight": {
      "title": "turnRight",
      "forms": [{
        "href": "action/turnRight",
        "contentType": "application/json"
      }]
    },
    "turnLeft": {
      "title": "turnLeft",
      "forms": [{
        "href": "action/turnLeft",
        "contentType": "application/json"
      }]
    },
    "goStraight": {
      "title": "goStraight",
      "forms": [{
        "href": "action/goStraight",
        "contentType": "application/json"
      }]
    },
    "goHome": {
      "name": "goHome",
      "idempotent": true,
      "forms": [{
        "href": "action/goHome",
        "contentType": "application/json"
      }]
    }
  }
}
